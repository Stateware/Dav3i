{
	"options" : {
		"banner" : "<%= banner %>",
		"stripBanners" : "true"
	},
	"internal" : {
		"src" : ["<%= pkg.scripts.internalLib %>"],
		dest : "prod/<%= pkg.name %>.js"
	},
	"index" : {
		"src" : ["<%= pkg.scripts.index %>"],
		"dest" : "prod/index.js"
	},
	"appWideCss" : {
		"src" : ["<%= pkg.css.appWide %>"],
		"dest" : "prod/<%= pkg.name %>.css"
	},
	"indexCss" : {
		"src" : ["<%= pkg.css.index %>"],
		"dest" : "prod/index.css"
	}
}
